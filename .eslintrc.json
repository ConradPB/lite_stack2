{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "prettier",
     "airbnb-base"],

    "plugins": ["prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"

    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
              "singleQuote": true,
              "parser": "flow",

            }
          ]
    }
}
